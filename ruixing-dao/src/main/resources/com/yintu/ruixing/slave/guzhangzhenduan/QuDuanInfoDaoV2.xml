        select ${shuxingname}, qi.createtime
        from ${tableName} qi,
             mro_quduan_base qb
        where qi.v1 = qb.qdid
          and qb.quDuanYunYing_name = "${quduanname}"
          and qi.v1 = "${qdid}"
          and qi.createtime
            BETWEEN
            "${starttime}"
            AND
            "${endtime}"
