    <select id="findDianMaHuaDatasByCZids" resultMap="quduanBaseResultMap">
        SELECT da.createtime time,
        da.type,
        da.v1 qid,
        da.v2 data_zhengChang,
        da.v3 V_out_zhu,
        da.v9 V_out_bei,
