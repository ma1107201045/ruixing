        da.v7 Hz_low_zhu,
        da.v13 Hz_low_bei,
        da.v8 FBJ_drive_zhu,
        da.v14 FBJ_drive_bei,
        da.v39_sc3 FBJ_collection_zhu,
        da.v39_sc2 FBJ_collection_bei
        FROM ${tableName} da
        WHERE da.v1 IN
        <foreach collection="qids" open="(" close=")" index="index" item="qid" separator=",">
