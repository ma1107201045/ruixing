<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yintu.ruixing.dao.ListDao">
    
    <select id="getListTieLuJuAll" resultType="com.yintu.ruixing.entity.TieLuJuEntity">
          select * from mro_tieluju;
    </select>

    <select id="getDianWuDuan" resultType="com.yintu.ruixing.entity.DianWuDuanEntity">
          select dwd.dwd_name,dwd.tlj_id from mro_dianwuduan dwd where dwd.tlj_id=#{tlj_id}
    </select>

    <select id="getXianDuan" resultType="com.yintu.ruixing.entity.XianDuanEntity">
          select xd.xd_name,xd.dwd_id from mro_xianduan xd where xd.dwd_id=#{dwd_id}
    </select>

    <select id="getCheZhan" resultType="com.yintu.ruixing.entity.CheZhanEntity">
          select cz.cz_name,cz.xd_id from mro_chezhan cz where cz.xd_id=#{xd_id}
    </select>
</mapper>
