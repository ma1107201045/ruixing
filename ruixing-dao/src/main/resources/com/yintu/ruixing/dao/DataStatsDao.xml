    <!--根据铁路局id 查询铁路局下的所有车站-->
    <select id="findTieLuJuById" resultType="com.yintu.ruixing.entity.DataStatsEntity" parameterType="java.lang.Long"
            resultMap="dataStatEntities">
        <!--SELECT
        tlj.tlj_name,
        tlj.tlj_miaoshu,
        dwd.dwd_name
        FROM
        mro_tieluju tlj
        left join mro_dianwuduan dwd
        on tlj.tid = dwd.tlj_dwd_id
