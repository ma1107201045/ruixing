    <!--查詢所有的车站数据-->
    <select id="findAll" resultType="com.yintu.ruixing.entity.DataStatsEntity" resultMap="dataStatEntities">
     SELECT
		tlj.tlj_name,
		dwd.dwd_name,
		xd.xd_name,
		cz.*
	FROM
		mro_chezhan cz,
		mro_dianwuduan dwd,
		mro_tieluju tlj,
		mro_xianduan xd
	WHERE
		tlj.id = dwd.tlj_dwd_id
		AND dwd.id = xd.dwd_xd_id
		AND xd.id = cz.xd_cz_id

    </select>

    <!--根据id批量删除车站-->
    <delete id="delCheZhanListById" parameterType="java.util.List">
        delete from mro_chezhan
        where cid
        in
        <foreach collection="array" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <!--查询所有车站信息-->
    <select id="findAllCheZhan" resultMap="dataStatEntities">
            SELECT
                tlj.tlj_name,
                dwd.dwd_name,
                xd.xd_name,
                xd.xd_state,
                xd.xd_json,
                xd.xid,
                tlj.tlj_id,
                dwd.dwd_id,
                xd.xd_id,
                cz.*
            FROM
                mro_tieluju tlj,
                mro_dianwuduan dwd,
                mro_xianduan xd,
                mro_chezhan cz
            WHERE
                tlj.tid = dwd.tlj_dwd_id
                AND dwd.did = xd.dwd_xd_id
                AND xd.xid = cz.xd_cz_id
            ORDER BY
                tlj.tlj_name
    </select>
