        SELECT
            qd.id,
            qd.carrier,
            qd.quDuanYunYing_name,
            qd.dianMaHuaGuiHao,
            qd.guiNeiDiZhi
        FROM
            mro_line li,
            mro_quduan_base qd
        WHERE
            li.id = qd.line_id
            and
            li.cid=#{id}
    </select>

