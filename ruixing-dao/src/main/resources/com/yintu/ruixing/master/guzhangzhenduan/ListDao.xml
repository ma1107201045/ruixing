    <select id="selectDwdListBytId" parameterType="long" resultMap="dianwuduan">
        SELECT mro_dianwuduan.did,
               mro_dianwuduan.dwd_id,
               mro_dianwuduan.tlj_id,
               mro_dianwuduan.tlj_dwd_id,
